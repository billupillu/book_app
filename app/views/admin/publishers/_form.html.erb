<div class="col-md-6 col-md-offset-3" style="text-align:center">
<%=bootstrap_form_for([:admin, @publisher], :html=> {class: 'form-horizontal'}) do |f| %>
<div class="form-group">
  <%= f.text_field :name, class: "form-control",hide_label: true,id: "publisherName", placeholder: "enter Name (ex: ORielly Printings)" %>
</div>


<br />

<div class="form-group actions control-group">
  
    <%=f.submit class: "btn btn-success"%>
  
</div>
<%end%>
</div>
